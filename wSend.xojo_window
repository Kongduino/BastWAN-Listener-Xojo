#tag Events pbSend
	#tag Event
		Sub Action()
		  Dim js As New JSONItem
		  
		  js.Value("from")=tfFrom.Text.Trim()
		  js.Value("cmd")="msg"
		  js.Value("msg")=tfMessage.Text.Trim()
		  tfMessage.Text=""
		  Self.Hide()
		  
		  Dim mb As MemoryBlock
		  
		  mb = js.ToString
		  Window1.log(mb)
		  If cbEncrypt.Value Then
		    Dim key As MemoryBlock
		    key=tfPassword.Text.Trim()
		    mb=AES_Encrypt(mb, key)
		  End If
		  
		  Window1.hexDump(mb)
		  
		  SendOneMessage(mb)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer1
	#tag Event
		Sub Action()
		  If tfFrom.Text.Trim()="" Then
		    pbSend.Enabled=False
		    Return
		  End If
		  
		  If tfMessage.Text.Trim()="" Then
		    pbSend.Enabled=False
		    Return
		  End If
		  
		  If tfPassword.Text.Trim().Length<>32 And cbEncrypt.Value Then
		    pbSend.Enabled=False
		    Return
		  End If
		  
		  pbSend.Enabled=True
		End Sub
	#tag EndEvent
#tag EndEvents
